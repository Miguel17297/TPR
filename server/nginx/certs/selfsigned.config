# Para gerar usar o comando: openssl req -new -nodes -x509 -days 365 -keyout nginx.key -out nginx.crt -config selfsigned.config

[req]
default_bits = 2048
default_md = sha256
distinguished_name = req_distinguished_name
x509_extensions = v3_req
prompt = no
[req_distinguished_name]
C = PT
ST = Aveiro
L = SomeCity
O = UA
OU = nginx-server
CN = nginx-server
[v3_req]
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth
subjectAltName = @alt_names
[alt_names]
IP.1	 = 127.0.0.1
DNS.1	 = localhost
DNS.2	 = myserver.io
